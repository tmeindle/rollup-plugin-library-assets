{
  "name": "rollup-plugin-library-assets",
  "version": "0.1.0",
  "description": "A rollup plugin for including component library assets such as stylsheets, fonts and images",
  "author": "Timothy Meindle",
  "license": "CC0-1.0",
  "main": "dist/index.cjs",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "engines": {
    "node": ">=18.12.0",
    "pnpm": ">=9.12.0"
  },
  "packageManager": "pnpm@9.12.2",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs",
      "default": "./dist/index.mjs"
    }
  },
  "files": [
    "LICENSE",
    "README.md",
    "dist"
  ],
  "scripts": {
    "rollup": "rollup --config ./rollup.config.mjs",
    "lint": "eslint --fix",
    "test": "vitest"
  },
  "dependencies": {
    "colorette": "2.0.20",
    "css-url-parser": "1.1.4",
    "fs-extra": "11.2.0",
    "p-limit": "6.1.0",
    "postcss": "8.4.47",
    "postcss-url": "10.1.3",
    "postcss-scss": "4.0.9",
    "rollup-pluginutils": "2.8.2",
    "sass": "1.79.4"
  },
  "devDependencies": {
    "@eslint/js": "9.13.0",
    "@vitest/coverage-v8": "2.1.3",
    "eslint": "9.13.0",
    "eslint-config-prettier": "9.1.0",
    "eslint-plugin-prettier": "5.2.1",
    "globals": "15.11.0",
    "prettier": "3.3.3",
    "rollup": "4.24.0",
    "vitest": "^2.1.3"
  }
}
